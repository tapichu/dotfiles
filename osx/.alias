# My aliases

alias ..='cd ..'
alias ...='cd ../..'
alias l='ls -alF'
alias la='ls -la'
alias ll='ls -l'
alias lld='ll --group-directories-first'
alias grep='grep --color=auto'
alias mutt='cd ~/Desktop && mutt'

alias gvim='mvim'
alias gvimdiff='mvimdiff'

alias wlst='rlwrap /opt/oracle/wls1034/wlserver_10.3/common/bin/wlst.sh'

